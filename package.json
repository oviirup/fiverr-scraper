{
  "name": "fiverr-scraper",
  "version": "3.0.2",
  "type": "module",
  "displayName": "Fiverr Scraper",
  "description": "Simple tool to manually scrape Fiverr gig, for future references or chat-bots",
  "homepage": "https://github.com/oviirup/fiverr-scraper",
  "author": "Avirup Ghosh (https://github.com/oviirup)",
  "license": "MIT",
  "scripts": {
    "build": "plasmo build",
    "clean": "npx rimraf .plasmo build",
    "dev": "plasmo dev",
    "format": "prettier . -w -u --cache"
  },
  "dependencies": {
    "plasmo": "~0.90.3",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "selector-observer": "~2.1.6"
  },
  "devDependencies": {
    "@types/bun": "~1.2.8",
    "@types/chrome": "~0.0.312",
    "@types/react": "~19.0.12",
    "@types/react-dom": "~19.0.4",
    "autoprefixer": "~10.4.21",
    "postcss": "~8.5.3",
    "prettier": "~3.5.3",
    "prettier-plugin-imports": "~4.2.12",
    "prettier-plugin-jsdoc": "~1.3.2",
    "prettier-plugin-pkg": "~0.18.1",
    "prettier-plugin-tailwindcss": "~0.6.11",
    "tailwindcss": "~3.4.17",
    "typescript": "~5.8.2"
  },
  "manifest": {
    "permissions": [
      "tabs",
      "storage"
    ],
    "host_permissions": [
      "https://*/*"
    ]
  }
}
