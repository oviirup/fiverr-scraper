{
  "name": "fiverr-scraper",
  "version": "3.0.2",
  "type": "module",
  "displayName": "Fiverr Scraper",
  "description": "Simple tool to manually scrape Fiverr gig, for future references or chat-bots",
  "homepage": "https://github.com/oviirup/fiverr-scraper",
  "author": "Avirup Ghosh (https://github.com/oviirup)",
  "license": "MIT",
  "scripts": {
    "build": "plasmo build",
    "clean": "npx rimraf .plasmo build",
    "dev": "plasmo dev",
    "format": "prettier . -wu --cache",
    "lint": "eslint src"
  },
  "dependencies": {
    "@plasmohq/messaging": "~0.7.1",
    "@plasmohq/storage": "~1.15.0",
    "plasmo": "~0.90.3",
    "react": "~19.1.0",
    "react-dom": "~19.1.0",
    "selector-observer": "~2.1.6"
  },
  "devDependencies": {
    "@eslint/js": "~9.23.0",
    "@types/bun": "~1.2.8",
    "@types/chrome": "~0.0.313",
    "@types/react": "~19.1.0",
    "@types/react-dom": "~19.1.1",
    "@types/turndown": "~5.0.5",
    "autoprefixer": "~10.4.21",
    "eslint": "~9.23.0",
    "eslint-config-prettier": "~10.1.1",
    "eslint-plugin-only-warn": "~1.1.0",
    "eslint-plugin-react": "~7.37.4",
    "eslint-plugin-react-hooks": "~5.2.0",
    "eslint-plugin-tailwindcss": "~3.18.0",
    "postcss": "~8.5.3",
    "prettier": "~3.5.3",
    "prettier-plugin-imports": "~4.2.12",
    "prettier-plugin-jsdoc": "~1.3.2",
    "prettier-plugin-pkg": "~0.19.0",
    "prettier-plugin-tailwindcss": "~0.6.11",
    "tailwindcss": "~3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "manifest": {
    "permissions": [
      "tabs",
      "storage",
      "webRequest"
    ],
    "host_permissions": [
      "https://*/*",
      "https://www.fiverr.com/*"
    ]
  }
}
